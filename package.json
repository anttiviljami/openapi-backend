{
  "name": "openapi-backend",
  "description": "Build, Validate, Route, and Mock using OpenAPI specification. Framework-agnostic",
  "version": "1.7.2",
  "author": "Viljami Kuosmanen <viljami@avoinsorsa.fi>",
  "license": "MIT",
  "keywords": [
    "openapi",
    "swagger",
    "server",
    "router",
    "validation",
    "mock",
    "express",
    "hapi",
    "koa",
    "serverless",
    "lambda",
    "azure"
  ],
  "homepage": "https://github.com/anttiviljami/openapi-backend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttiviljami/openapi-backend.git"
  },
  "bugs": {
    "url": "https://github.com/anttiviljami/openapi-backend/issues"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "!*.test.*",
    "!**/*.test.*",
    "!scripts",
    "!node_modules",
    "!examples",
    "!src",
    "!*.config.js"
  ],
  "dependencies": {
    "ajv": "^6.7.0",
    "bath": "^2.1.1",
    "cookie": "^0.3.1",
    "lodash": "^4.17.11",
    "mock-json-schema": "^1.0.5",
    "openapi-schema-validation": "^0.4.2",
    "openapi-types": "^1.3.2",
    "qs": "^6.6.0",
    "swagger-parser": "^6.0.3"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.2",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.18",
    "@types/qs": "^6.5.1",
    "@types/swagger-parser": "^4.0.2",
    "jest": "^23.6.0",
    "markdown-toc": "^1.2.0",
    "prettier": "^1.15.3",
    "source-map-support": "^0.5.10",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.2.4"
  },
  "scripts": {
    "build": "tsc",
    "watch-build": "tsc -w",
    "lint": "tslint --format prose --project .",
    "prettier": "prettier --write src/** __tests__/**",
    "toc": "./scripts/generate-toc.js",
    "test": "NODE_ENV=test jest"
  }
}
